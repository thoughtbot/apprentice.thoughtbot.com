:@template"y"body {
  border-top: 4px solid #ad141e;
  color: #333;
}

section#container {
  /*background: url(../images/995-grid.png) repeat-y;*/
  @extend .clearfix;
  margin: 0 auto;
  padding: 30px 0 100px;
  width: grid-width(12);

  header {
    @extend .clearfix;
    background: url("/assets/giantrobots/images/logo.png") 31px 16px no-repeat;
    border-bottom: 1px solid #ddd;
    margin: 0 0 $lh*2 0;
    padding: 0 0 $lh*3 0;

    hgroup {
      float: left;
      padding: 40px 0 0 grid-width(2) + $gutter;
      width: grid-width(7);

      h1 {
        font: 700 12px/14px $sans-serif;
        margin: 0 0 6px 0;
        text-transform: uppercase;
      }

      h2 {
        color: #777;
        font: 400 13px/16px $serif;
        font-style: italic;
      }
    }

    nav {
      @extend .clearfix;
      float: left;
      margin: 0 0 0 $gutter;
      padding: 40px 0 0 0;
      width: grid-width(3);

      ul {
        @extend .clearfix;
        margin: 0 15px 0 0;

        li {
          float: right;
          width: 40px;

          a {
            display: block;
            height: 41px;
          }

          &#twitter {
            a {
              background: url("../images/twitter.png") -8px -1px no-repeat;
              text-indent: -9999px;
              @include transition(0.5s);

              &:hover {
                background: url("../images/twitter.png") -8px -48px no-repeat;
              }
            }
          }
        }
      }
    }
  }

  aside {
    float: left;
    margin: 10px 0 0 $gutter;
    width: grid-width(3);

    .message {
      a {
        background: #e9e9e9;
        border: 1px solid #dedede;
        @include border-radius(3px);
        color: #333;
        display: block;
        padding: 15px;
        text-decoration: none;
        @include transition(0.5s);

        img {
          float: left;
          margin: 0 15px 0 0;
        }

        h3 {
          color: #777;
          font: 400 14px/14px $serif;
          font-style: italic;
          margin: 0 0 4px 0;
          text-decoration: none;
        }

        h4 {
          color: #333;
          font: 700 16px/16px $sans-serif;
          text-decoration: none;
        }

        &:hover {
          @include border-radius(0);
          @include box-shadow(0 0 0 8px rgba(0, 0, 0, 0.07));
        }
      }
    }
  }

  section#posts {
    float: left;
    width: grid-width(9);

    .post {
      border-bottom: 3px solid #dddddd;
      margin: 0 0 $lh*3 0;
      padding: 0 0 $lh*1 grid-width(2) +  $gutter;
      position: relative;

      .meta-info {
        left: 10px;
        position: absolute;
        top: 18px;
        width: grid-width(2);

        p.post-date {
          font: 400 14px/14px $serif;
          margin: 0 0 $lh*2 0;
        }

        p.tagged {
          font: 600 12px/12px $serif;
          margin: 0 0 10px 0;
          text-transform: uppercase;
        }

        ul.tags {
          color: #777;

          li {
            font: 400 13px/13px $serif;
            font-style: italic;
            margin: 0 0 10px 0;
            position: relative;

            a {
              color: #777;
              font-weight: normal;
              text-decoration: none;
            }

            &:after {
              content: "\2014";
              font-size: 13px;
              left: -14px;
              position: absolute;
              top: 0;
            }
          }
        }
      }

      h2, h3 {
        color: #111;
        font: 700 14px/21px $sans-serif;
        margin: 20px 0 0px 0;
        text-transform: uppercase;
      }

      p {
        font: 400 14px/22px $sans-serif;
        margin: 0 0 12px 0;
      }

      blockquote {
        padding: 0 30px 0 30px;

        p {
          color: #555;
          font-style: italic;
        }
      }

      ul {
        margin: 0 0 ($lh - 4) 0 ;

        li {
          font: 400 14px/22px $sans-serif;
          margin: 0 0 4px 0;
          position: relative;

          &:after {
            content: "\2022";
            font-size: 18px;
            font-weight: 700;
            left: -12px;
            position: absolute;
            top: -2px;
          }
        }
      }

      img {
        max-width: grid-width(7);
      }

      a {
        color: $red;
        font-weight: bold;
        text-decoration: none;

        &:hover {
         text-decoration: underline
        }
      }

      em {
        font-style: italic;
      }

      section.post-footer {
        p.author {
          font: 400 14px/14px $serif;
          font-style: italic;
          left: -12px;
          position: relative;
          /*top: 26px;*/

          &:before {
            content: "\2014";
          }
        }

        p.comments {
          font: 400 14px/14px $serif;
          left: -(grid-width(2) + 15);
          position: relative;
          top: 26px;
          width: grid-width(2);

          a {
            color: #333;
            font-weight: 400;
          }
        }
      }

      /* text posts */
      h2.title {
        font: 300 36px/38px $sans-serif;
        margin: 0 0 30px 0;
        text-transform: none;
      }

      /* quote posts */
      &.quote {
        .meta-info {
          top: 8px;
        }

        q {
          font: 400 18px/26px $sans-serif;
          position: relative;

          &:before {
            content: '\201C';
            color: #333;
            font-size: 36px;
            position: absolute;
            top: 4px;
            left: -20px;
          }

          & q:before {
            content: '\2018';
          }

          & q:after {
            content: '\2019';
          }
        }

        p.source {
          left: -16px;
          margin: 10px 0 0 0;
          position: relative;
        }

        &.medium,
        &.long {
          q {
            font: 400 14px/22px $sans-serif;
          }
        }

        .post-footer {
          p.comments {
            padding-bottom: 20px;
          }
        }
      }
    }
  }
}

/* Pagination
------------------------------------------------------------------------------*/
#pagination {
  padding: 0 0 $lh*3 (grid-width(2) + $gutter);
  width: grid-width(7);
}

#pagination a {
  color: #444;
  font: 700 13px/13px $sans-serif;
  padding: 6px 9px;
  text-decoration: none;
  text-transform: uppercase;
}

#pagination a:hover {
  color: #000;
}

#pagination #older-link {
  float: left;
  margin: 0 0 0 0;
  padding: 15px 0 0 0;
  width: grid-width(2);
}

#pagination #older-link a {
  background: url("../images/arrows.png") 0 10px no-repeat;
  margin: 0 0 0 -3px;
}

#pagination #newer-link {
  float: right;
  margin: 0 0 0 0;
  padding: 15px 0 0 0;
  text-align: right;
  width: grid-width(2);
}

#pagination #newer-link a {
  background: url("../images/arrows.png") right -18px no-repeat;
  margin: 0 -3px 0 0;
}

/* Footer
------------------------------------------------------------------------------*/
footer {
  background: #efefef;
  border-top: 1px solid #e0e0e0;
  padding: $lh*3 0 $lh*6;

  section#footer-container {
    @extend .clearfix;
/*    background: url(../images/995-grid.png) repeat-y;*/
    margin: 0 auto;
    width: grid-width(12);

    h6 {
      color: #4a4a4a;
      font: 300 18px/18px $sans-serif;
      margin: 0 0 $lh 0;
    }

    nav {
      float: left;
      margin-right: $gutter;
      width: grid-width(4);

      ul {
        float: left;
        width: grid-width(2);

        &#secondary-links {
          margin-left: $gutter;
        }

        li {
          color: #7a7a7a;
          font: 400 14px/18px $serif;
          margin: 0 0 6px 0;

          a {
            color: #7a7a7a;
            text-decoration: none;
          }
        }
      }
    }

    section#products {
      @extend .clearfix;
      float: left;
      width: grid-width(8);

      section.product {
        float: left;
        margin-right: $gutter;
        width: grid-width(2);

        a.logo {
          background: #e9e9e9;
          border: 1px solid #dedede;
          @include border-radius(3px);
          display: block;
          height: 100px;
          padding: 15px 0;
          text-align: center;
          @include transition(0.4s);

          &:hover {
            @include border-radius(0);
            @include box-shadow(0 0 0 6px rgba(0, 0, 0, 0.04));
          }
        }

        &#hoptoad a.logo {
          padding: 18px 0 14px 0;
        }

        &#copycopter a.logo {
          padding: 25px 0 5px 0;

          img {
            width: 110px;
          }
        }

        &#trajectory a.logo {
          height: 90px;
          padding: 40px 0 0px 0;

          img {
            width: 110px;
          }
        }

        &#radish {
          margin-right: 0;
        }
      }
    }
  }
